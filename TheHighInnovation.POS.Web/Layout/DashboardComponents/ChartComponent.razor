@inject IJSRuntime _JS;




@* <canvas id="myChart" width="300" height="50"></canvas> *@
<canvas id="myChart"></canvas>



@code {

    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            await _JS.InvokeVoidAsync("initializeChart");
        }
    }
}


